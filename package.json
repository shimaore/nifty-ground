{
  "name": "ccnq3_traces",
  "version": "0.0.1",
  "description": "SIP sniffer traces",
  "keywords": "",
  "author": { "name": "Stephane Alnet", "email": "stephane@shimaore.net" },
  "config": {
    "file": "/etc/ccnq3/host.json"
  },
  "scripts": {
    "start": "./start.coffee; daemon -n ccnq3_trace_server -o daemon.debug -D \"`pwd`/agents\" -r -- ./agent.coffee"
