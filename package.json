  },
  "dependencies": {
    "ccnq3_config": ">=0.3.0"
  , "pico": ">=0.0.22"
  , "byline": ">=2.0.3"
  },
  "engines": { "node": ">=0.6.6", "coffee": ">=1.1.2" },

  "private": true,
  "ok": true
}
