{
  "name": "ccnq3_traces",
  "version": "0.0.1",
  "description": "SIP sniffer traces",
  "keywords": "",
  "author": { "name": "Stephane Alnet", "email": "stephane@shimaore.net" },
  "config": {
    "config_file": "/etc/ccnq3/host.json"
  },
  "scripts": {
    "start": "./start.coffee; daemon -n ccnq3_trace_server -o daemon.debug -D \"`pwd`/agents\" -r -- ./agent.coffee"
  , "stop": "./stop.coffee; daemon -n ccnq3_trace_server -o daemon.debug --stop"
  },
  "dependencies": {
    "ccnq3_config": ">=0.1"
  , "pico": ">=0.0.11f"
  },
  "engines": { "node": ">=0.6.6", "coffee": ">=1.1.2" },

  "private": true,
  "ok": true
}
